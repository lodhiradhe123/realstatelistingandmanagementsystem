<%-include("./partials/header") %>
<%-include("./partials/nav") %>

<div class="w-full h-screen bg-pink-300 text-center p-10 items-center flex gap-10">
  <% property.forEach((ele)=>{%>
    <div class="owner w-1/3 h-96 bg-zinc-300 rounded-lg p-5">
        <div class="profile w-full h-32 rounded-lg bg-pink-300  overflow-hidden">
            <img src="/images/<%=ele.image%>" alt="" class="w-full h-full object-cover">
        </div>
        <h1 class="text-3xl uppercase font-bold text-start"><%=ele.title%></h1>
        <h1 class="text-2xl uppercase font-bold text-start"><%=ele.description%></h1>
        <h1 class="text-3xl uppercase font-bold text-start text-yellow-200">Rs-<%=ele.price%></h1>
        <h1 class="text-2xl uppercase font-bold text-start">Loc..-<%=ele.location%></h1>
        <h1 class="text-2xl uppercase font-bold text-start text-blue-500">status-<%=ele.status%></h1>
        <h1 class="text-3xl uppercase font-bold text-start inline wrap">owner</h1>
    </div>
   <%})%>

</div>
<%-include("./partials/footer") %>